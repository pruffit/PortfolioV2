		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(rel="stylesheet", href="assets/styles/main.min.css")
		title #{title}
	body
		.wrapper
			main.maincontent
				block content
		script(src="assets/scripts/main.min.js")